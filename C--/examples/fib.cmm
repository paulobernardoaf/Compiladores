void Fibonacci(int value) {

    int a = 1;
    int b = 1;
    int next;

    if value == 0 {
        return;
    }

    during next <= value {

        print("$d, ", a);
        next = a + b;
        a = b;
        b = next;

    }

    print("$d\n", a);

    return;
}

int Main() {

    int value;

    get(value);

    Fibonacci(value);

    return 0;
}